
<div class='contents'>
  <div class='middle_content'>
    <h3>投稿してみよう</h3>
    <%= form_for(@article) do |f|%>
       <p>タイトル</p>
       <%= f.text_field :title, class: 'title_input', placeholder: 'タイトルを入力してください', size: 35 %>
      <p>画像</p>
      <%= f.file_field :image_url %>
      

      <p>本文</p>
      <%= f.text_area :content, placeholder: '本文を入力してください', size: "70x14", class: "content_input" %>
      <%= f.submit "送信する", class: 'submit_button' %>

      <% end %>
  </div>
</div>
